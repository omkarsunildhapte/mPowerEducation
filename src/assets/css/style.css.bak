body {
  margin: 0;
  padding: 0;
}
.profilepic-loading {
  text-align: center;
  position: relative;
  right: 1rem;
  float: right;
  width: 8.7rem;
  padding: 5px;
  border-radius: 5px;
  box-shadow: 0px 0px 5px #00000038;
}
.clear {
  clear: both;
}
.btn-round {
  border-radius: 2rem;
  width: 100px;
}
.profilepic-loading > img {
  width: 100%;
}
.billdesk-icon {
  width: 120px;
}
.container-fluid-bg {
  background-image: url("../images/svu-img-1.png");
  background-repeat: no-repeat;
  background-origin: padding-box;
  background-position: center;
}
body {
  background-image: url('../background.jpg');
  background-size: cover;
  background-repeat: no-repeat;
}
header {
  background-color: #fff;
  padding: 20px;
  color: #fff;
}

